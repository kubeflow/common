    "operator/v1/zz_generated.deepcopy.go",
    "operator/v1/zz_generated.defaults.go",
    "operator/v1/openapi_generated.go",
    "test_job/v1/zz_generated.deepcopy.go",
    "test_job/v1/zz_generated.defaults.go",
    "test_job/v1/openapi_generated.go"
  ],
  "Deadline": "600s",
  "Skip": ["client"]
}
