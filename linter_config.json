{
  "Vendor": true,
  "DisableAll": true,
  "Enable": [
    "vet",
    "safesql",
    "errcheck",
    "goconst",
    "goimports",
    "varcheck",
    "gosec",
    "gotype",
    "staticcheck",
    "lll",
    "unconvert",
    "misspell",
    "unconvert"
  ],
  "Aggregate": true,
  "WarnUnmatchedNolint": true,
  "LineLength": 240,
